<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_users_tem_form_view" model="ir.ui.view">
        <field name="name">res.users.maryun.t.form.view</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='preferences']" position="after">
                <group string="Payment Term" >
                    <field name="change_payment_method" />
                </group>
            </xpath>
        </field>
    </record>
</odoo>
